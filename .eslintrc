{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "rules": {
    "no-prototype-builtins": 0,
    "no-unused-vars": [
      2,
      {
        "ignoreRestSiblings": true
      }
    ],
    "prefer-spread": 0
  }
}
