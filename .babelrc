{
  "env": {
    "development": {
      "plugins": ["syntax-flow", "transform-flow-strip-types"],
      "presets": [
        [
          "env",
          {
            "exclude": ["transform-es2015-typeof-symbol"],
            "loose": true,
            "modules": false
          }
        ],
        "stage-2"
      ]
    },
    "es": {
      "plugins": ["syntax-flow", "transform-flow-strip-types"],
      "presets": [
        [
          "env",
          {
            "exclude": ["transform-es2015-typeof-symbol"],
            "loose": true,
            "modules": false
          }
        ],
        "stage-2"
      ]
    },
    "lib": {
      "plugins": [
        "syntax-flow",
        "transform-flow-strip-types",
        [
          "search-and-replace",
          [
            {
              "search": "micro-memoize/es/utils",
              "replace": "micro-memoize/lib/utils"
            }
          ]
        ]
      ],
      "presets": [
        [
          "env",
          {
            "exclude": ["transform-es2015-typeof-symbol"],
            "loose": true
          }
        ],
        "stage-2"
      ]
    },
    "mjs": {
      "plugins": [
        "syntax-flow",
        "transform-flow-strip-types",
        [
          "search-and-replace",
          [
            {
              "search": "micro-memoize/es/utils",
              "replace": "micro-memoize/mjs/utils"
            },
            {
              "search": "micro-memoize",
              "replace": "micro-memoize/mjs"
            },
            {
              "search": "fast-equals",
              "replace": "fast-equals/mjs"
            },
            {
              "search": "fast-stringify",
              "replace": "fast-stringify/mjs"
            }
          ]
        ]
      ],
      "presets": [
        [
          "env",
          {
            "exclude": ["transform-es2015-typeof-symbol"],
            "loose": true,
            "modules": false
          }
        ],
        "stage-2"
      ]
    },
    "production": {
      "plugins": ["syntax-flow", "transform-flow-strip-types"],
      "presets": [
        [
          "env",
          {
            "exclude": ["transform-es2015-typeof-symbol"],
            "loose": true,
            "modules": false
          }
        ],
        "stage-2"
      ]
    },
    "test": {
      "plugins": [
        "syntax-flow",
        "transform-flow-strip-types",
        [
          "search-and-replace",
          [
            {
              "search": "micro-memoize/es/utils",
              "replace": "micro-memoize/lib/utils"
            }
          ]
        ]
      ],
      "presets": [
        [
          "env",
          {
            "exclude": ["transform-es2015-typeof-symbol"],
            "loose": true
          }
        ],
        "stage-2"
      ]
    }
  }
}
