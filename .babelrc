{
  "plugins": [
    "syntax-flow",
    "transform-flow-strip-types",
    "add-module-exports"
  ],
  "presets": [
    ["env", {
      "loose": true,
      "modules": false
    }],
    "stage-2"
  ],
  "env": {
    "production": {
      "presets": [
        ["env", {
          "loose": true
        }],
        "stage-2"
      ]
    }
  }
}
